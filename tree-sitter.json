    "license": "MIT",
    "description": "Macaulay2 parser",
    "authors": [
      {
        "name": "Doug Torrance",
        "email": "dtorrance@piedmont.edu",
        "url": "https://webwork.piedmont.edu/~dtorrance"
      }
    ],
    "links": {
      "repository": "https://github.com/d-torrance/tree-sitter-macaulay2",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
